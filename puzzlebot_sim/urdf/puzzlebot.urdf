<?xml version="1.0"?>
<robot name="puzzlebot_team1">

  <link name="map"/>

  <link name="base_footprint"/>
  <joint name="map_to_base_footprint" type="fixed">
    <parent link="map"/>
    <child link="base_footprint"/>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
  </joint>

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://puzzlebot_sim/meshes/Puzzlebot_Jetson_Lidar_Edition_Base.stl" />
      </geometry>
      <material name="blue">
        <color rgba="0 0 0.8 1"/>
      </material>
    </visual>
  </link>
  <joint name="base_link_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
  </joint>

  <link name="wheel_r_link">
    <visual>
      <geometry>
        <mesh filename="package://puzzlebot_sim/meshes/Puzzlebot_Wheel.stl" />
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
  </link>
  <joint name="wheel_r_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_r_link"/>
    <origin xyz="0.052 -0.095 -0.0025" rpy="0.0 1.57 1.57"/>
    <axis xyz="0 0 1"/> </joint>

  <link name="wheel_l_link">
    <visual>
      <geometry>
        <mesh filename="package://puzzlebot_sim/meshes/Puzzlebot_Wheel.stl" />
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
  </link>
  <joint name="wheel_l_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_l_link"/>
    <origin xyz="0.052 0.095 -0.0025" rpy="0.0 -1.57 1.57"/>
    <axis xyz="0 0 1"/> </joint>

  <link name="caster_link">
    <visual>
      <geometry>
        <mesh filename="package://puzzlebot_sim/meshes/Puzzlebot_Caster_Wheel.stl" />
      </geometry>
      <material name="grey">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
  </link>
  <joint name="caster_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_link"/>
    <origin xyz="-0.095 0.0 -0.03" rpy="0.0 0.0 0.0"/>
  </joint>

</robot>